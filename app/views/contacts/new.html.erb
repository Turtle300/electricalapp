<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="well">
      <%= form_for @contact do |f| %>
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :phone %>
          <%= f.text_field :phone, class: 'form-control' %>
        </div>
        <div class="form-group">
          <%= f.label :comments %>
          <%= f.text_area :comments, class: 'form-control' %>
        </div>
        <%= f.submit 'Submit', class: 'btn btn-default' %>
      <% end %>
    </div>
  </div>
</div>
<div class ="jumbotron">
  <div class="row">
    <div class="col-md-4">
      <h3>Call us at:<br>970-232-8979</h2>
    </div>
    <div class="col-md-4">
      <h3>Address:<br> 945 Campfire Drive, Fort Collins, CO 80524</h2>
    </div>
    <div class="col-md-4">
      <h3>Email: thelectrician@outlook.com</h2>
    </div>
  </div>
</div>